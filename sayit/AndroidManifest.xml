<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.visibolic.sayit"
      android:versionCode="1"
      android:versionName="1.0"
      >
       <uses-sdk android:minSdkVersion="11" android:targetSdkVersion="11"/>
       <uses-permission android:name="android.permission.INTERNET" />
       <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
       <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <application 
        android:theme="@android:style/Theme.Holo.Light" 
        android:label="@string/app_name" 
        android:icon="@drawable/mouth_rest">
        <activity android:name=".SayIt"
                  android:label="@string/app_name">
            <intent-filter>
	            <action android:name="android.intent.action.MAIN" />
    	        <category android:name="android.intent.category.LAUNCHER" />
        	     <category android:name="android.intent.category.BROWSABLE" />
        	</intent-filter>
        <intent-filter>
            <action android:name="android.intent.action.VIEW" />
            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
            
            
        </activity>
		<activity
            android:label="Result Activity"
            android:name="com.visibolic.sayit.activities.ResultActivity" >
        </activity>
        <activity
            android:label="Settings Activity"
            android:name="com.visibolic.sayit.activities.SettingsActivity" >
        </activity>
        
        <activity
            android:label="Webviewer Activity"
            android:name="com.visibolic.sayit.activities.WebviewActivity" >
        </activity>
         
        </application>


</manifest> 